  	<extension point="org.eclipse.jdt.core.classpathVariableInitializer">
  	   <classpathVariableInitializer
            class="com.ibm.ast.ws.jaxws.emitter.util.CPVInitializer"
            readOnly="true"
            variable="WAS_V85JAXWS_WEBSERVICES_THINCLIENT"/>
